{
  "name": "midiflip",
  "version": "0.3.0",
  "description": "Transform MIDI files mathematically",
  "author": "Isaiah Odhner <isaiahodhner@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1j01/midiflip.git"
  },
  "bugs": {
    "url": "https://github.com/1j01/midiflip/issues"
  },
  "keywords": [
    "midi",
    "flip",
    "midiflip",
    "transform",
    "midi-file",
    "midi-events",
    "remix",
    "remixer",
    "remixes",
    "music"
  ],
  "main": "src/midiflip.js",
  "bin": {
    "midiflip": "src/cli.js"
  },
  "dependencies": {
    "glob": "^7.1.1",
    "midifile": "^1.0.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "scripts": {
    "do-single": "midiflip -i node_modules/midifile/sounds/Avgvst/Danger.mid -o transformed.mid",
    "do": "midiflip -i \"node_modules/midifile/sounds/*/*.mid\" -o \"./transformed/\"",
    "do-mess-with-percussion": "midiflip -i \"node_modules/midifile/sounds/*/*.mid\" -o \"./transformed/\" -p"
  }
}
